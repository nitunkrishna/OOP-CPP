#include<iostream>

using namespace std;

class object
{
    int id;
public:
    void get_info(void)
    {
        cout << "Enter id of object : ";
        cin >> id;
    }
    void show_info(void)
    {
        cout << "Object : "<< id << endl;
    }
};

int main()
{
    object ob[5];
    for(int i=0; i<5; i++)
    {
        ob[i].get_info();
        ob[i].show_info();
    }
    return 0;
}
